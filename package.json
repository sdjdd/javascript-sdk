{
  "name": "leancloud-storage",
  "version": "5.0.0",
  "main": "./dist/node/entry/index.js",
  "description": "LeanCloud JavaScript SDK.",
  "repository": {
    "type": "git",
    "url": "https://github.com/leancloud/javascript-sdk"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:core && npm run build:platform",
    "build:core": "rollup -c rollup/core.config.js",
    "build:platform": "npm run build:node",
    "build:node": "rollup -c rollup/node.config.js",
    "doc": "typedoc --out docs src",
    "test": "mocha",
    "coverage": "nyc npm run test",
    "repl": "ts-node ./tools/repl",
    "lint": "eslint --ext=ts,js --max-warnings=0 .",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@types/superagent": "^4.1.7",
    "base64-arraybuffer": "^0.2.0",
    "debug": "^4.1.1",
    "eventemitter3": "^4.0.4",
    "superagent": "^5.2.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^3.1.1",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^6.8.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "prettier": "2.0.5",
    "rollup": "^2.12.1",
    "should": "^13.2.3",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.3",
    "websocket": "^1.0.31"
  },
  "license": "MIT",
  "author": {
    "name": "LeanCloud",
    "email": "support@leancloud.rocks"
  }
}
